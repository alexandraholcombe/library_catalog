@{
    Layout="Shared/_Layout.cshtml";
}

<h1>All patrons</h1>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Patron Name</th>
      <th>Another thing</th>
    </tr>
  </thead>
  <tbody>
    @{
      foreach(var patron in Model)
      {
        <tr>
        <td>@patron.GetLastName(), @patron.GetFirstName()</td>
        <td>favorite color</td>
        </tr>
      }
    }
  </tbody>
</table>

<p><a href="/">Go Home</a></p>
